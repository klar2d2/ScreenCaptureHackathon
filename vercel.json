{
  "version": 2,
  "name": "my-fullstack-app",
  "builds": [
    { "src": "api/upload.js", "use": "@vercel/node" },
    { "src": "client/package.json", "use": "@vercel/static-build",  "config": {
      "distDir": "build"
    } }
  ],
  "routes": [
    { "src": "/api/*(.*)", "dest": "/api/upload.js" },
    { "src": "/(.*)", "dest": "/client/$1" }
  ]
}
