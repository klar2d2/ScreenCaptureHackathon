{
  "version": 2,
  "name": "my-fullstack-app",
  "builds": [
    { "src": "api/server.js", "use": "@vercel/node" },
    { "src": "client/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/server.js" },
    { "src": "/(.*)", "dest": "/client/build/$1" }
  ]
}
